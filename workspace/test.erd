erdiagram Webshop2
generateSql

entity Person {
    person_id: int key 
    age: string
}


entity SpecificCustomer extends Person {
    customer_id: int key
}

entity entity5 {
    customer_id: int key
	address: string optional
}

entity Invoice {
    invoice_id key
    price: double foreign-key
    orderDate: datetime
    deliveryDate: datetime

    amount: int
}

entity Product {
    product_id: int key
    productName: string
    amount: int
} 

entity Test4 {
    testId: int key
}

entity Customer {
    customer_id: int key
}

entity weak Payment {
    payment_number: int partial-key
}

relationship weak pays {
    Customer[1] -> Payment[N]
}

//entity Test5 extends Test4

relationship name {
    Test4['abcd'] -> entity5[1]
}

relationship self{
    Person[1] -> Person[1]
}

relationship abcdef {
    Person[1] -> entity5[N]
}

relationship Purchase1 {
    entity5[1] -> Product[N]
    customer_id: int foreign-key
    product_id: int foreign-key
}

relationship gets {
    entity5[1] -> Invoice[N] -> Payment[N]
}